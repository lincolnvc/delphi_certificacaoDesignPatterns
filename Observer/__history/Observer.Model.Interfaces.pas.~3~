unit Observer.Model.Interfaces;

interface

type
  TEvDisplay = procedure (Value : String) of object;

  TPedido = record
    Produto : String;
    Setor : String;
  end;

  iObserver = interface
    ['{161EB732-5581-4341-9C81-ED1BBBFE96F5}']
    function Update(Value : TPedido) : iObserver;
  end;



implementation

end.
