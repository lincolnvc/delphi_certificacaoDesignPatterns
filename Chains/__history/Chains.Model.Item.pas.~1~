unit Chains.Model.Item;

interface

Type
  TModelItem = class(TInterfacedObject, iMinhaClasse)
    private
    public
      constructor Create;
      destructor Destroy; override;
      class function New : iMinhaClasse;
  end;

implementation

{ TModelItem }

constructor TModelItem.Create;
begin

end;

destructor TModelItem.Destroy;
begin

  inherited;
end;

class function TModelItem.New: iMinhaClasse;
begin
  Result := Self.Create;
end;

end.
